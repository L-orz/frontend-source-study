{
  "name": "bi-frontend",
  "version": "0.0.0",
  "scripts": {
    "lint": "npm run eslint && npm run prettier",
    "prettier": "prettier \"./src/**/*\" --check",
    "prettier:fix": "prettier --write -l \"./src/**/*\"",
    "eslint": "eslint --ext .js,.ts --ignore-pattern \"bad.*\" ./src",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-alloy": "^4.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.4.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  }
}
